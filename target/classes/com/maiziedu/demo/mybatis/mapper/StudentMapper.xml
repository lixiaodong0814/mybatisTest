<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.maiziedu.demo.mybatis.mapper.StudentMapper">
	
	<resultMap id="selectStudentResult" type="Student1">
		<result property="id" column="id"/>
		<result property="name" column="name"/>
		<result property="birthday" column="birthday"/>
		<result property="gender" column="gender" javaType="String" jdbcType="INTEGER"/>
	</resultMap>
	
	<!--Student 别名   java.lang.Long 为long-->
	<select id="selectStudent" parameterType="long" resultMap="selectStudentResult">
		select * from Student where ID = #{id}
	</select>
</mapper>